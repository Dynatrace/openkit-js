{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "linterOptions": {
        "exclude": [
            "src/**/*"
        ]
    },
    "jsRules": {},
    "rules": {
        "interface-name": [true, "never-prefix"],
        "unified-signatures": [false],
        "max-line-length": [true, 140],
        "quotemark": [true, "single"],
        "object-literal-sort-keys": [false]
    },
    "rulesDirectory": []
}
