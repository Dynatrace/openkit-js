{
  "name": "@dynatrace/openkit-js",
  "version": "0.0.1",
  "description": "OpenKit-JavaScript reference implementation",
  "scripts": {
    "test": "jest",
    "lint": "tslint --project ./tsconfig.base.json -c tslint.json",
    "lint:spec": "tslint --project ./tsconfig.base.json -c tslint.spec.json",
    "docs": "typedoc --out docs src/OpenKitBuilder.ts src/api --theme markdown",
    "build": "yarn build:browser && yarn build:node",
    "build:node": "tsc -p tsconfig.node.json",
    "build:browser": "webpack --mode production",
    "build:browser:dev": "webpack"
  },
  "author": "Dynatrace",
  "types": "dist/types/index.d.ts",
  "main": "dist/node/index.js",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "@types/promise-polyfill": "^6.0.1",
    "codelyzer": "^4.5.0",
    "jest": "^24.1.0",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "ts-mockito": "^2.3.1",
    "tslint": "^5.12.1",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.1.25",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "yarn": "^1.13.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "es6-promise": "4.2.6"
  },
  "files": [
    "dist",
    "docs"
