{
  "name": "@dynatrace/openkit-js",
  "version": "0.0.2",
  "description": "OpenKit-JavaScript reference implementation",
  "author": "Dynatrace",
  "types": "dist/types/index.d.ts",
  "main": "dist/node/index.js",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dynatrace/openkit-js"
  },
  "scripts": {
    "test": "jest",
    "lint": "tslint --project ./tsconfig.base.json -c tslint.json",
    "lint:spec": "tslint --project ./tsconfig.base.json -c tslint.spec.json",
    "docs": "typedoc --out docs src/OpenKitBuilder.ts src/index.ts src/api --tsconfig tsconfig.base.json --exclude src/core/**/* --excludePrivate --excludeNotExported",
    "docs:markdown": "yarn docs --theme markdown",
    "build": "yarn build:browser && yarn build:node",
    "build:node": "tsc -p tsconfig.node.json",
    "build:browser": "webpack --mode production",
    "build:browser:dev": "webpack"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/promise-polyfill": "^6.0.3",
    "codelyzer": "^5.0.0-beta.1",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "ts-mockito": "^2.3.1",
    "tslint": "^5.14.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.1.27",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "yarn": "^1.13.0"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "files": [
    "dist",
    "docs"
  ]
}

